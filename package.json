  "packageManager": "pnpm@10.14.0",
  "description": "Web3 Identity Resolver · Web3 Identity Kit · Web3 Domain Name Service Resolver",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
