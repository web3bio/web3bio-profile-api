{
  "name": "web3bio-profile-api",
  "version": "0.4.3",
  "packageManager": "pnpm@10.26.2",
  "description": "Web3 Identity Resolver · Web3 Identity Kit · Web3 Domain Name Service Resolver",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --testPathIgnorePatterns=\"/__tests__/(ns|profile)/ud.test.js$\"",
    "test:all": "jest",
    "lint": "eslint .",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --remote --port 3000",
    "staging": "opennextjs-cloudflare build && wrangler versions upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.13.1",
    "graphql": "^16.11.0",
    "highlight.js": "11.11.1",
    "jose": "^5.10.0",
    "next": "15.5.9",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "viem": "^2.39.3",
    "web3bio-profile-kit": "^0.1.37"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "20.12.8",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "dotenv": "17.2.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.9",
    "jest": "^29.7.0",
    "sass": "^1.89.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wrangler": "^4.50.0"
  }
