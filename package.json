    "web3bio-profile-kit": "^0.1.21"
  },
  "devDependencies": {
    "wrangler": "^4.27.0",
    "@eslint/eslintrc": "^3.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "20.12.8",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.4",
    "jest": "^29.7.0",
    "sass": "^1.89.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
